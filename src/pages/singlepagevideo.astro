---
import "../styles/global.css";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import SinglepageSektion from "../components/SinglepageSektion.astro";
const {} = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link
      rel="icon"
      type="image/svg+xml"
      href="/favicon.svg"
    />
    <link
      rel="stylesheet"
      href="https://use.typekit.net/lhr7bio.css"
    />
    <meta
      name="viewport"
      content="width=device-width"
    />
    <meta
      name="generator"
      content={Astro.generator}
    />

    <meta
      name="description"
      content="Du har altid adgang til vores hjemmevideoer, til de dage hvor du ikke når ned i centeret"
    />
    <title>Hjemmetræning video</title>
  </head>
  <body class="body-bg">
    <Header />

    <SinglepageSektion
      titel="Boldtræning"
      info1="varihed: 20 minutter"
      info2="alle kan være med"
      info3="min 4 deltagere"
      beskrivelse="Hvis du har 20 minutter, så kom og vær med på denne ‘følg mig’ træning, hvor du får brugt hele kroppen, og får godt med sved på panden. Fysisk Forms fysioterapeuter og personlige trænere instruerer og viser de øvelser du skal igennem, og laver hver eneste gentagelse sammen med dig – du skal blot følge med så godt du kan."
      knap={false}
      knaptekst=""
    />
    <Footer />
  </body>
</html>

<style></style>
